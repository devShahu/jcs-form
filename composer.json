{
    "name": "jcs/pdf-form-filler",
    "description": "JCS Membership Form PDF Generator",
    "type": "project",
    "require": {
        "php": ">=8.0",
        "intervention/image": "^2.7",
        "slim/psr7": "^1.6",
        "slim/slim": "^4.11",
        "spatie/browsershot": "^5.1",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "api/src/",
            "App\\Controllers\\": "api/controllers/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
